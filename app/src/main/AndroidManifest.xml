<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.sitirohtiani.pesonalebak">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.main.MainActivity"/>
        <activity android:name=".views.login.LoginActivity"/>
        <activity android:name=".views.detail.DetailActivity"/>
        <activity android:name=".views.map.MapActivity"/>
        <activity android:name=".views.objek.ObjekActivity"/>
        <activity android:name=".views.destinasi.DestinasiActivity"/>
        <activity android:name=".views.objekwisata.ObjekWisataActivity"/>
    </application>
</manifest>